<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="600" maxWidth="800" minHeight="600" minWidth="800" prefHeight="600.0" prefWidth="800.0"
            style="-fx-background-color: grey;" stylesheets="application.css" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.workoutmaker.Controller">
    <children>
        <Label layoutX="14.0" layoutY="100.0" text="Workout Schedule">
            <font>
                <Font size="26.0"/>
            </font>
        </Label>
        <Line endX="130.0" layoutX="115.0" layoutY="138.0" startX="-100.0" strokeWidth="2.0"/>
        <Label layoutX="43.0" layoutY="315.0" text="Monday" AnchorPane.leftAnchor="43.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Label layoutX="152.0" layoutY="314.0" text="Tuesday" AnchorPane.leftAnchor="152.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Label layoutX="250.0" layoutY="314.0" text="Wednesday" AnchorPane.leftAnchor="250.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Label layoutX="367.0" layoutY="314.0" text="Thursday" AnchorPane.leftAnchor="367.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Label layoutX="488.0" layoutY="314.0" text="Friday" AnchorPane.leftAnchor="488.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Label layoutX="588.0" layoutY="314.0" text="Saturday" AnchorPane.leftAnchor="588.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Label layoutX="702.0" layoutY="314.0" text="Sunday" AnchorPane.leftAnchor="702.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Button layoutX="663.0" layoutY="14.0" mnemonicParsing="false" onAction="#selectSplit" text="Select Split">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToMain" text="Back">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
        <Label layoutX="14.0" layoutY="185.0" text="SPLIT:">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <Label fx:id="splitNameLabel" layoutX="83.0" layoutY="185.0">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <ListView fx:id="mondayListView" layoutX="22.0" layoutY="344.0" prefHeight="156.0" prefWidth="99.0"/>
        <ListView fx:id="tuesdayListView" layoutX="132.0" layoutY="344.0" prefHeight="156.0" prefWidth="99.0"/>
        <ListView fx:id="wednesdayListView" layoutX="242.0" layoutY="344.0" prefHeight="156.0" prefWidth="99.0"/>
        <ListView fx:id="thursdayListView" layoutX="350.0" layoutY="344.0" prefHeight="156.0" prefWidth="99.0"/>
        <ListView fx:id="fridayListView" layoutX="460.0" layoutY="344.0" prefHeight="156.0" prefWidth="99.0"/>
        <ListView fx:id="saturdayListView" layoutX="570.0" layoutY="344.0" prefHeight="156.0" prefWidth="99.0"/>
        <ListView fx:id="sundayListView" layoutX="680.0" layoutY="344.0" prefHeight="156.0" prefWidth="99.0"/>
        <Button layoutX="592.0" layoutY="178.0" mnemonicParsing="false" onAction="#loadSelectedSplit"
                text="Load Selected Split">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
